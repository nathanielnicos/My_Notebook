<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.nns.myjetpackcompose">

    <application
        android:name=".MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyJetpackCompose">
        <activity
            android:name=".ui.edit_note.EditNoteActivity"
            android:exported="true"
            android:label="@string/title_activity_edit_note"
            android:theme="@style/Theme.MyJetpackCompose.NoActionBar" />
        <activity
            android:name=".ui.add_note.AddNoteActivity"
            android:exported="true"
            android:label="@string/title_activity_add_note"
            android:theme="@style/Theme.MyJetpackCompose.NoActionBar" />
        <activity
            android:name=".ui.main.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyJetpackCompose.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>